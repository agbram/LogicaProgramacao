programa {
  funcao inicio() {

    inteiro num, divisao, sobra

    escreva("Escreva o valor a ser dividido por 3:")
    leia(num)

    divisao = num / 3
    sobra = num % 3

    escreva("\nO valor da divisão de ",num," por 3 é ", divisao)
    escreva("\nA sobra da divisão é ", sobra)

  




    
  }
}
